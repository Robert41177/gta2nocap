{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          'Chrome >= 60',
          'Safari >= 10.1',
          'iOS >= 10.3',
          'Firefox >= 54',
          'Edge >= 15',
        ]
      },
      "exclude": [
        "transform-regenerator"
      ]
    }]
  ],
  "env": {
    "production": {
      "plugins": [
        "babel-plugin-transform-remove-console"
      ]
    }
  }
}
